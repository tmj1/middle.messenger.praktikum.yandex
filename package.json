{
  "name": "Messenger Sprint 3",
  "version": "1.0.0",
  "description": "---",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack serve",
    "start": "npm run build && node server.js",
    "lint:ts": "npx eslint .",
    "lint:style": "npx stylelint \"**/*.css\" --fix",
    "docker": "docker build -t messanger .",
    "prepare": "husky install",
    "test": "mocha"
  },
  "alias": {
    "core": "./src/core",
    "pages": "./src/pages",
    "components": "./src/components",
    "img": "./src/image",
    "routes": "./src/routes",
    "types": "./src/types",
    "vendor": "./src/vendor",
    "variables": "./src/variables",
    "utils": "./src/utils",
    "styles": "./src/styles",
    "api": "./src/api",
    "services": "./src/services"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmj1/Sprint-3"
  },
  "author": "Tatiana Miteva <tmj1@gmail.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tmj1/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/tmj1/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/node": "^18.11.9",
    "@types/jest": "28.1.7",
    "babel-jest": "28.1.3",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "autoprefixer": "10.4.8",
    "babel": "^6.23.0",
    "babel-loader": "8.2.5",
    "chai": "^4.3.7",
    "clean-webpack-plugin": "4.0.0",
    "core-js": "3.24.1",
    "css-loader": "6.7.1",
    "cssnano": "5.1.13",
    "eslint": "^8.28.0",
    "handlebars": "^4.7.7",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "mocha": "^10.2.0",
    "postcss-loader": "7.0.1",
    "postcss-nested": "^5.0.6",
    "stylelint": "^14.15.0",
    "stylelint-config-standard": "^29.0.0",
    "ts-loader": "9.3.1",
    "ts-jest": "28.0.8",
    "identity-obj-proxy": "3.0.0",
    "typescript": "^4.9.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.10.0"
    },
  "dependencies": {
    "docker": "1.0.0",
    "express": "^4.18.2",
    "express-handlebars": "^6.0.6",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "jest-environment-jsdom": "28.1.3",
    "nanoid": "^4.0.0",
    "webpack": "5.74.0",
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/(?!nanoid)"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "^core(.*)$": "<rootDir>/src/core$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^types(.*)$": "<rootDir>/src/types$1",
      "^pages(.*)$": "<rootDir>/src/pages$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^img(.*)$": "<rootDir>/src/img$1",
      "^routes(.*)$": "<rootDir>/src/routes$1",
      "^vendor(.*)$": "<rootDir>/src/vendor$1",
      "^api(.*)$": "<rootDir>/src/api$1",
      "^services(.*)$": "<rootDir>/src/services$1"
    }
  }
}
